    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "run:e2e": "set NODE_ENV=test&&start-server-and-test test http://localhost:4000 cypress:run",
    "open:e2e": "start-server-and-test test http://localhost:4000 cypress:open"
