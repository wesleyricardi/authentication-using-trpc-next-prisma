    "@trpc/server": "^10.0.0-proxy-alpha.78",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.5.1",
    "next": "12.3.0",
    "nodemailer": "^6.7.8",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
